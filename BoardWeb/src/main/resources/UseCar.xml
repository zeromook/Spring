<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="car" class="practice.Volvo" init-method="On" destroy-method="off" >
	<constructor-arg ref="Analog"></constructor-arg>
</bean>

<bean id="Smart" class="practice.SmartKey" lazy-init="true"></bean>
<bean id="Analog" class="practice.AnalogKey" lazy-init="true"></bean>

</beans>
